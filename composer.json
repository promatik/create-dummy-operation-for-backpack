{
    "name": "promatik/create-dummy-operation",
    "description": "CreateDummyOperation adds a button to your CRUDs to create dummy entries based on your model factories",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Almeida",
            "email": "promatik@gmail.com",
            "homepage": "https://promatik.pt"
        }
    ],
    "homepage": "https://github.com/promatik/createdummyoperation",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "CreateDummyOperation"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Promatik\\CreateDummyOperation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Promatik\\CreateDummyOperation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Promatik\\CreateDummyOperation\\AddonServiceProvider"
            ]
        }
    }
}
